[
  {
    "name": "mikes-app-container",
    "image": "644237782704.dkr.ecr.us-east-2.amazonaws.com/mikes_app:latest",
    "memory": 512,
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 8080
      }
    ],
    "environment": [
      {
        "name": "DB_HOST",
        "value": "${DB_HOST}"
      },
      {
        "name": "DB_PORT",
        "value": "${DB_PORT}"
      },
      {
        "name": "DB_NAME",
        "value": "${DB_NAME}"
      },
      {
        "name": "DB_USER",
        "value": "${DB_USER}"
      },
      {
        "name": "DB_PASSWORD",
        "value": "${DB_PASSWORD}"
      }
    ]
  }
]